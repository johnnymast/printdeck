{
  "SDKVersion": 2,
  "Author": "Johnny Mast",
  "CodePath": "plugin/index.html",
  "PropertyInspectorPath": "inspector/index.html",
  "Description": "See the progress of your 3d prints via Octoprint on your stream deck",
  "Category": "Print Deck",
  "CategoryIcon": "Images/categoryIcon",
  "Name": "Print Deck",
  "Icon": "octoprint-logo",
  "URL": "https://github.com/johnnymast/printdeck",
  "Version": "1.0",
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion" : "10.11"
    },
    {
      "Platform": "windows",
      "MinimumVersion" : "10"
    }
  ],
  "Software":
  {
    "MinimumVersion" : "4.1"
  },
  "Actions": [
    {
      "Icon": "action/images/actionProgressImage",
      "Name": "Show Progress",
      "States": [
        {
          "Image": "action/images/keyProgressImage"
        }
      ],
      "SupportedInMultiActions": false,
      "Tooltip": "Progress",
      "UUID": "com.johnnymast.printdeck.progress"
    },
    {
      "Icon": "action/images/actionInstanceImage",
      "Name": "Open Octoprint",
      "States": [
        {
          "Image": "action/images/keyInstanceImage"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Open OctoPrint",
      "UUID": "com.johnnymast.printdeck.openportal"
    }
  ]
}