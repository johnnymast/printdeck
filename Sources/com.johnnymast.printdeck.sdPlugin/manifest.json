{
  "SDKVersion": 2,
  "Name": "Print Deck",
  "Author": "Johnny Mast",
  "CodePath": "plugin/index.html",
  "PropertyInspectorPath": "inspector/index.html",
  "Description": "See the progress of your 3d prints via Octoprint on your stream deck",
  "Category": "Print Deck",
  "CategoryIcon": "Images/categoryIcon",
  "Icon": "images/octoprint-logo",
  "URL": "https://github.com/johnnymast/printdeck",
  "Version": "1.0",
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion" : "10.11"
    },
    {
      "Platform": "windows",
      "MinimumVersion" : "10"
    }
  ],
  "Software":
  {
    "MinimumVersion" : "4.1"
  },
  "Actions": [
    {
      "Icon": "action/images/actionProgressImage",
      "Name": "Show Print progress",
      "Tooltip": "Show the OctoPrint progression on your Stream Deck",
      "States": [
        {
          "Image": "action/images/keyProgressImage"
        }
      ],
      "SupportedInMultiActions": false,
      "UUID": "com.johnnymast.printdeck.octoprint_progress"
    },
    {
      "Icon": "action/images/actionInstanceImage",
      "Name": "Open Octoprint",
      "Tooltip": "Open your OctoPrint page in your web browser.",
      "States": [
        {
          "Image": "action/images/keyInstanceImage"
        }
      ],
      "SupportedInMultiActions": true,
      "UUID": "com.johnnymast.printdeck.octoprint_openaction"
    },
    {
      "Icon": "action/images/actionTemperatureImage",
      "Name": "Set temperature",
      "Tooltip": "Set the temperature for your printer.",
      "States": [
        {
          "Image": "action/images/keyTemperatureImage"
        }
      ],
      "SupportedInMultiActions": true,
      "UUID": "com.johnnymast.printdeck.octoprint_set_temperature"
    },
    {
      "Icon": "action/images/actionCoolDownImage",
      "Name": "Cooldown",
      "Tooltip": "Cool down your printer.",
      "States": [
        {
          "Image": "action/images/keyCooldownImage"
        }
      ],
      "SupportedInMultiActions": true,
      "UUID": "com.johnnymast.printdeck.octoprint_cooldown"
    }
  ]
}